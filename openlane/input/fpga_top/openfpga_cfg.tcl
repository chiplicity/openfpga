#####################
# Specify Grid Size 
# and Macro Sizes
#####################

set grid_x 2
set grid_y 2

# grid clb
set clb_x 250
set clb_y 250

# Switch 
set sb_x 140
set sb_y 140

# xbar
set cbx_x 150
set cbx_y 120

set cby_x 80
set cby_y 200

# IOs (same size as xbars)
set io_hor_x $cbx_x
set io_hor_y 120

set io_ver_x 80
set io_ver_y $io_hor_x

set io_ver_to_sb 15
set io_hor_to_sb 15

set EMBED_IOS 1